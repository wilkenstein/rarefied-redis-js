{
    "name": "rarefied-redis-js",
    "author": "Brian Wilke <wilke.brian@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git://github.com/wilkenstein/rarefied-redis-js.git"
    },
    "main": "rarefied-redis.js",
    "version": "0.0.1-3",
    "devDependencies": {
        "redis-js": "0.0.9-5",
        "redis": "~0.12",
        "q": "~1.0",
        "chai": "~2.1",
        "chance": "0.6.1",
        "complexity-report": "1.0.2",
        "docco": "*",
        "istanbul": "0.3.5",
        "jshint": "2.3.0",
        "karma": "~0.12",
        "karma-chai": "^0.1.0",
        "karma-firefox-launcher": "~0.1",
        "karma-chrome-launcher": "~0.1",
        "karma-ie-launcher": "~0.1",
        "karma-safari-launcher": "~0.1",
        "karma-opera-launcher": "~0.1",
        "karma-mocha": "^0.1.10",
        "mocha": "1.13.0",
        "node-jsmeter": "0.1.2",
        "plato": "0.6.2",
        "should": "~5.0",
        "uglify-js": "2.4.x",
        "jsdoc": "~3.2"
    },
    "scripts": {
        "pretest": "node_modules/.bin/jshint rarefied-redis.js util/*.js reliable/*.js concurrency/*.js",
        "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -R spec -t 60s -s 5s test/mocha",
        "posttest": "node_modules/.bin/plato -d plato/ rarefied-redis.js adapter/*.js util/*.js reliable/*.js concurrency/*.js",
        "test-integrations": "REDIS_JS_TO_NODE_REDIS=1 node_modules/.bin/_mocha -R spec -t 60s -s 5s test/mocha",
        "test-phantomjs": "npm i karma-phantomjs-launcher && ./node_modules/karma/bin/karma start",
        "test-firefox": "./node_modules/karma/bin/karma start --browsers Firefox",
        "test-chrome": "./node_modules/karma/bin/karma start --browsers Chrome",
        "test-ie": "./node_modules/karma/bin/karma start --browsers IE",
        "test-safari": "./node_modules/karma/bin/karma start --browsers Safari",
        "test-opera": "./node_modules/karma/bin/karma start --browsers Opera",
        "doc": "./node_modules/.bin/jsdoc --destination docs/ rarefied-redis.js util/*.js queue/*.js reliable/*.js crypto/*.js"
    },
    "license": "MIT",
    "files": [
        "rarefied-redis.js",
        "util/redis-util.js",
        "util/redis-uuid.js",
        "util/redis-expirer.js",
        "util/redis-scanner.js",
        "adapter/redis-list-adapter.js",
        "concurrency/redis-cas.js",
        "concurrency/redis-concurrency.js",
        "reliable/redis-reliable-locker.js",
        "reliable/redis-reliable-deleter.js",
        "reliable/redis-reliable-mover.js",
        "reliable/redis-reliable-consumer.js",
        "reliable/redis-reliable-producer.js",
        "package.json",
        "README.md",
        "LICENSE"
    ],
    "keywords": [
        "redis",
        "rarefied",
        "rarefied-redis",
        "Rarefied Redis",
        "rarefied redis",
        "abstraction",
        "abstractions"
    ]
}
